/**about.wxss**/
@import "../../../assets/icon/iconfont.wxss";

.page {
  font-family: '微软雅黑';
  background: #fff;
  display: flex;
  flex-direction: column;
  width: 100vw;
  height: 100vh
}

.title {
  color: #f8fbfe;
  font-size: 42rpx;
  width: 180rpx;
  height: 60rpx;
  background: #2f7ad5;
  position: absolute;
  top: 90rpx;
  left: 38%;
  z-index: 9999;
}

.title1 {
  color: #f8fbfe;
  font-size: 30rpx;
  height: 60rpx;
  width: 100%;
  position: absolute;
  top:200rpx;
  left: 10%;
  z-index: 9999;
}

.header-box {
  height: 380rpx;
  width: 100%;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  position: relative;
}

.content-box {
  color: #979797;
  font-family: PingFangSC-Regular;
  letter-spacing: 0.5px;
  line-height: 1.5;
  font-size: 24rpx;
  padding: 20rpx;
  background-color: #fff;
}

.content {
  margin-top: 15rpx;
  padding: 41rpx 41rpx 10rpx 41rpx;
  display: flex;
  flex-direction: column;
}

.wx-cell {
  margin-bottom: 30rpx;
  margin-left: 16rpx;
  position: relative;
  display: flex;
}

/* 覆盖按钮默认样式 */

.btn {
  height: 100%;
  width: 100%;
  border: none;
  border-radius: 0;
  padding: 0;
  margin: 0;
  font-size: 1em;
  position: static;
  /* color: rgba(0, 0, 0, 0.65); */
  text-align: left;
  background: #fff;
}

.btn::after {
  content: none;
}

.login-out {
  margin-top: 20rpx;
  text-align: center;
  background-color: #2f7ad5;
  padding: 25rpx;
  margin: 20rpx;
  border-radius: 20rpx;
  color: #fff;
  font-size: 34rpx;
  letter-spacing: 2rpx;
}

.icon {
  font-size: 45rpx;
  margin-top: 15rpx;
  margin-right: 15rpx;
}

.icon1 {
  font-size: 45rpx;
  margin-right: 15rpx;
}
